        "laravel",
        "eloquent-to-raw-sql",
        "to-raw-sql"
    ],
    "homepage": "https://github.com/f-liva/eloquent-to-raw-sql",
    "license": "MIT",
    "authors": [
        {
            "name": "Federico Liva",
            "email": "mail@federicoliva.dev",
            "role": "Developer"
        },
        {
            "name": "Jan OÌˆstlund",
            "email": "janne@eastwest.se",
            "role": "Developer"
        }
