    "require": {
        "doctrine/sql-formatter": "^1.3",
        "illuminate/contracts": "*"
    },
    "autoload": {
        "psr-4": {
            "Fliva\\EloquentToRawSql\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
